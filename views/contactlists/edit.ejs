<% include ../partials/header%>

<div class="container">
	<div class="row">
<h1 style="text-align:center">Edit:<%=contactlist.first_name%></h1>
<form action="/contactlists/<%= contactlist._id %>/?_method=PUT" method="POST">
	<div class="form-group">
	<input type="text" name="contactlist[first_name]" value="<%=contactlist.first_name%>">
	</div>
	<div class="form-group">
	<input type="text" name="contactlist[last_name]" placeholder="<%=contactlist.last_name%>"">
	<div class="form-group">
	<input type="text" name="contactlist[address]" placeholder="<%=contactlist.address%>"">
	<div class="form-group">
	<input type="text" name="contactlist[city]" placeholder="<%=contactlist.city%>"">
	<div class="form-group">
	<input type="text" name="contactlist[state]" placeholder="<%=contactlist.state%>">
	<div class="form-group">
	<input type="text" name="contactlist[zip]" placeholder="<%=contactlist.zip%>">
	<div class="form-group">
	<input type="email" name="contactlist[email]" placeholder="<%=contactlist.email%>">
	<input type="text" name="contactlist[phone]" placeholder="<%=contactlist.phone%>">
	<button class="btn btn-lg btn-primary btn-block">Submit</button>
	
</form>
 <a href="/contactlists">Go Back</a>
</div>
</div>
<% include ../partials/footer%>